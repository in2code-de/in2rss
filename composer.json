        "rss",
        "TYPO3"
    ],
    "homepage": "https://github.com/in2code-de/in2glossar",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sandra Pohl",
            "email": "sandra.pohl@in2code.de",
            "homepage": "https://www.in2code.de",
            "role": "Product owner"
        },
        {
            "name": "Alex Kellner",
            "email": "alexander.kellner@in2code.de",
            "homepage": "https://www.in2code.de",
            "role": "Technical owner"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-simplexml": "*",
        "typo3/cms-core": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "In2code\\In2rss\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "in2rss"
        }
    }
}
